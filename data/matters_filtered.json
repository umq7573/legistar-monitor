    "MatterEnactmentNumber": "2023/122",
    "MatterRequester": null,
    "MatterNotes": "{\\rtf1\\ansi\\ansicpg1252\\deff0\\deflang1033{\\fonttbl{\\f0\\fnil\\fcharset0 Arial;}}\r\n\\viewkind4\\uc1\\pard\\fs20 Local Law 122 of 2023\\par\r\n}",
